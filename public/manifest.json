{
  "name": "デジキャン拡張",
  "version": "1.1.2",
  "description": "Digicam extension built by okatechnology",
  "manifest_version": 2,
  "homepage_url": "https://github.com/okatechnology/digicam-extension",
  "permissions": ["declarativeContent", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [{
    "matches": ["http://portal.dhw.ac.jp/*", "https://portal.dhw.ac.jp/*"],
    "js": ["script.js"],
    "css": ["custom.css"]
  }],
  "icons": {
    "128": "digicamExtensionLogo128.png",
    "48": "digicamExtensionLogo48.png",
    "16": "digicamExtensionLogo16.png"
  },
  "key": ""
}
