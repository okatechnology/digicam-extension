{
  "name": "Digicam extensions",
  "version": "1.0",
  "description": "Digicam extensions built by okatechnology",
  "manifest_version": 2,
  "permissions": ["declarativeContent", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [{
    "matches": ["http://portal.dhw.ac.jp/*", "https://portal.dhw.ac.jp/*"],
    "js": ["script.js"],
    "css": ["custom.css"]
  }],
  "icons": {
    "128": "digicamExtensionLogo128.png",
    "48": "digicamExtensionLogo48.png",
    "16": "digicamExtensionLogo16.png"
  }
}
